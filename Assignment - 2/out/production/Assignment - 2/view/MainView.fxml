<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainViewController">
   <children>
      <Label alignment="CENTER" layoutX="152.0" layoutY="14.0" prefHeight="82.0" prefWidth="295.0" text="Chatting system">
         <font>
            <Font name="Algerian" size="32.0" />
         </font>
      </Label>
      <TextField fx:id="messageField" layoutX="180.0" layoutY="96.0" prefHeight="54.0" prefWidth="238.0" promptText="Your message" />
      <Button layoutX="181.0" layoutY="165.0" mnemonicParsing="false" onAction="#numberButton" prefHeight="54.0" prefWidth="238.0" text="Request number of users" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button layoutX="181.0" layoutY="241.0" mnemonicParsing="false" onAction="#ipButton" prefHeight="54.0" prefWidth="238.0" text="See your own IP" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Label fx:id="responseLabel" alignment="CENTER" layoutX="181.0" layoutY="316.0" prefHeight="46.0" prefWidth="238.0" text="Response" textAlignment="CENTER" />
      <Button layoutX="434.0" layoutY="96.0" mnemonicParsing="false" onAction="#sendButton" prefHeight="46.0" prefWidth="87.0" text="SEND" />
   </children>
</AnchorPane>
